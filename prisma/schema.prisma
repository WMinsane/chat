generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model room {
  id      Int      @id @default(autoincrement())
  title   String
  description    String
  name    String
  date    String
}
model chat {
  id      Int      @id @default(autoincrement())
  roomId  Int
  text    String
  name    String
  date    String
}