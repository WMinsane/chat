generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Room {
  id      Int      @id @default(autoincrement())
  title   String
  description    String
  name    String
  date    String
}
model Chat {
  id      Int      @id @default(autoincrement())
  roomId  Int
  text    String
  name    String
  date    String
}